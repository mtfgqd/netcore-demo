{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Console": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "None",
      "Program": "Trace",
      "alogger": "Information",
      "alogger2": "Trace",
      "LoggingDemo.OrderService": "Trace",
      "LoggingDemo.WeatherForecastController": "Trace"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  }
}
